    "import requests\n",
    "import pandas as pd\n",
    "from datetime import datetime, timedelta\n",
    "import time\n",
    "import logging\n",
    "import boto3\n",
    "import io\n",
    "import os\n",
    "\n",
    "logging.basicConfig(level=logging.INFO, format='%(asctime)s - %(levelname)s - %(message)s')\n",
    "\n",
    "# Configuration\n",
    "cities = {\n",
    "    \"Chennai\": \"80.15,12.90,80.35,13.15\",\n",
    "#    \"Delhi\": \"76.84,28.40,77.34,28.88\",\n",
    "}\n",
    "#api_key = \"YOUR_API_KEY\"  # Replace with your actual API key\n",
    "NUMBER_OF_DAYS = 7  # 3 months = ~90 days\n",
    "NUM_SENSORS_TO_FETCH = 10\n",
    "S3_BUCKET_NAME = \"go01-demo\"  # Replace with your S3 bucket name\n",
    "# AWS_ACCESS_KEY_ID = \"YOUR_AWS_ACCESS_KEY_ID\"\n",
    "# AWS_SECRET_ACCESS_KEY = \"YOUR_AWS_SECRET_ACCESS_KEY\"\n",
    "\n",
    "headers = {\"X-API-Key\": api_key}\n",
    "# Initialize sessions\n",
    "authenticated_session = boto3.Session()  # For private bucket\n",
    "target_s3_client = authenticated_session.client('s3', region_name=REGION_NAME)\n",
    "s3 = boto3.client('s3', aws_access_key_id=AWS_ACCESS_KEY_ID, aws_secret_access_key=AWS_SECRET_ACCESS_KEY)\n",
    "all_city_data = {} #initialise the all_city_data dictionary\n",
    "\n",
    "def handle_rate_limit(response, sensor_id=None, city=None, wait_time=10):\n",
    "    \"\"\"Handles API rate limit errors.\"\"\"\n",
    "    if response.status_code == 429:\n",
    "        if sensor_id and city:\n",
    "            logging.warning(f\"Rate limit hit for sensor {sensor_id} in {city}. Waiting {wait_time} seconds...\")\n",
    "        else:\n",
    "            logging.warning(f\"Rate limit hit. Waiting {wait_time} seconds...\")\n",
    "        time.sleep(wait_time)\n",
    "        return True\n",
    "    return False\n",
    "\n",
    "for city, bbox in cities.items():\n",
    "    try:\n",
    "        locations_url = \"https://api.openaq.org/v3/locations\"\n",
    "        locations_params = {\"bbox\": bbox, \"limit\": 1000}\n",
    "        locations_response = requests.get(locations_url, params=locations_params, headers=headers)\n",
    "        # handles the API call rate of 50 API Calls/ minute throttling by sleeping between calls\n",
    "        if handle_rate_limit(locations_response):\n",
    "            locations_response = requests.get(locations_url, params=locations_params, headers=headers)\n",
    "        locations_response.raise_for_status()\n",
    "        locations_data = locations_response.json().get(\"results\", [])\n",
    "\n",
    "        sensor_data = {}\n",
    "        fetched_sensor_count = 0\n",
    "\n",
    "        for location in locations_data:\n",
    "            if fetched_sensor_count >= NUM_SENSORS_TO_FETCH:\n",
    "                break\n",
    "            sensors = location.get(\"sensors\", [])\n",
    "\n",
    "            for sensor in sensors:\n",
    "                if fetched_sensor_count >= NUM_SENSORS_TO_FETCH:\n",
    "                    break\n",
    "                sensor_id = sensor.get(\"id\")\n",
    "                print(\"SENSORID : \", sensor_id)\n",
    "                parameter_id = sensor.get(\"parameter\", {}).get(\"id\")\n",
    "                parameter_name = sensor.get(\"parameter\", {}).get(\"name\")\n",
    "\n",
    "                latest_measurement_url = f\"https://api.openaq.org/v3/sensors/{sensor_id}/hours/daily\" #modified API call\n",
    "                latest_measurement_params = {\"limit\": 1, \"sort\": \"desc\"}\n",
    "                latest_measurement_response = requests.get(latest_measurement_url, params=latest_measurement_params, headers=headers)\n",
    "                \n",
    "                # handles the API call rate of 50 API Calls/ minute throttling by sleeping between calls\n",
    "                if handle_rate_limit(latest_measurement_response, sensor_id, city, 60):\n",
    "                    latest_measurement_response = requests.get(latest_measurement_url, params=latest_measurement_params, headers=headers)\n",
    "                print(\" measurement response : \", latest_measurement_response.json())\n",
    "                latest_measurement_response.raise_for_status()\n",
    "                latest_measurement_data = latest_measurement_response.json().get(\"results\", [])\n",
    "\n",
    "                if latest_measurement_data and latest_measurement_data[0].get(\"period\", {}).get(\"datetimeFrom\", {}).get(\"utc\"): #modified to check for period.datetimeFrom.utc\n",
    "                    last_date_str = latest_measurement_data[0].get(\"period\", {}).get(\"datetimeFrom\", {}).get(\"utc\") #modified to get date from period.datetimeFrom.utc\n",
    "                    last_date = datetime.fromisoformat(last_date_str.replace('Z', '+00:00'))\n",
    "                    start_date = last_date - timedelta(days=NUMBER_OF_DAYS)\n",
    "                    start_date_str = start_date.isoformat().replace('+00:00', 'Z')\n",
    "                    end_date_str = last_date.isoformat().replace('+00:00', 'Z')\n",
    "                else:\n",
    "                    logging.warning(f\"No recent data for sensor {sensor_id} in {city}. Skipping.\")\n",
    "                    continue\n",
    "\n",
    "                measurements_url = f\"https://api.openaq.org/v3/sensors/{sensor_id}/hours/daily\" #modified API call\n",
    "                measurements_params = {\n",
    "                    \"date_from\": start_date_str,\n",
    "                    \"date_to\": end_date_str,\n",
    "                    \"limit\": 1000\n",
    "                }\n",
    "                measurements_response = requests.get(measurements_url, params=measurements_params, headers=headers)\n",
    "                if handle_rate_limit(measurements_response, sensor_id, city):\n",
    "                    measurements_response = requests.get(measurements_url, params=measurements_params, headers=headers)\n",
    "                measurements_response.raise_for_status()\n",
    "                measurements_data = measurements_response.json().get(\"results\", [])\n",
    "\n",
    "                if sensor_id not in sensor_data:\n",
    "                    sensor_data[sensor_id] = []\n",
    "\n",
    "                for measurement in measurements_data:\n",
    "                    sensor_data[sensor_id].append({\n",
    "                        \"date\": measurement.get(\"period\", {}).get(\"datetimeFrom\", {}).get(\"utc\"), #modified to get date from period.datetimeFrom.utc\n",
    "                        \"value\": measurement.get(\"value\"),\n",
    "                        \"unit\": measurement.get(\"parameter\", {}).get(\"units\"), #modified to get units from parameter.units\n",
    "                        \"location\": location.get(\"name\"),\n",
    "                        \"parameter_id\": parameter_id,\n",
    "                        \"parameter_name\": parameter_name\n",
    "                    })\n",
    "                fetched_sensor_count += 1\n",
    "                print(\"Sensor Count: \", fetched_sensor_count)\n",
    "        all_city_data[city] = sensor_data #store the sensor_data for each city in the dictionary\n",
    "\n",
    "    except requests.exceptions.RequestException as e:\n",
    "        logging.error(f\"Error fetching data for {city}: {e}\")\n",
